    <ns3:hasGenre rdf:resource="http://dbpedia.org/resource/Action_game"/>
    <ns3:name>Jumanji</ns3:name>
    <ns3:hasPlatform rdf:resource="http://dbpedia.org/resource/Personal_computer"/>
    <ns3:publisher rdf:resource="http://dbpedia.org/resource/Philips"/>
    <ns3:developer rdf:resource="http://dbpedia.org/resource/Studio_Interactive"/>
