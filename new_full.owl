  <rdf:Description rdf:about="http://dbpedia.org/resource/Mafia_Wars">
    <ns3:name>Mafia Wars</ns3:name>
    <ns3:hasGenre rdf:resource="http://dbpedia.org/resource/Role-playing_video_game"/>
    <ns3:hasPlatform rdf:resource="http://dbpedia.org/resource/Personal_computer"/>
    <ns3:developer rdf:resource="http://dbpedia.org/resource/Zynga"/>
    <ns3:hasPlatform rdf:resource="http://dbpedia.org/resource/IPhone"/>
    <ns3:hasGenre rdf:resource="http://dbpedia.org/resource/Casual_game"/>
