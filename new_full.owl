  <rdf:Description rdf:about="http://dbpedia.org/resource/Far_Cry_2">
    <ns3:hasPlatform rdf:resource="http://dbpedia.org/resource/Xbox_360"/>
    <ns3:media rdf:resource="http://dbpedia.org/resource/Optical_disc"/>
