    <ns3:hasGameEngine rdf:resource="http://dbpedia.org/resource/Arcanum:_Of_Steamworks_and_Magick_Obscura"/>
    <ns3:media rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</ns3:media>
