  <rdf:Description rdf:about="http://dbpedia.org/resource/Star_Wars_Jedi_Knight_II:_Jedi_Outcast">
    <ns3:hasPlatform rdf:resource="http://dbpedia.org/resource/Xbox_(console)"/>
