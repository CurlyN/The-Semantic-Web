  <rdf:Description rdf:about="http://dbpedia.org/resource/Fishing_Derby">
    <rdf:type rdf:resource="http://www.semanticweb.org/natalia/project1#VideoGame"/>
    <ns2:hasPlatform rdf:resource="http://dbpedia.org/resource/Atari_2600"/>
    <ns2:name>Fishing Derby</ns2:name>
