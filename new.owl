  <rdf:Description rdf:about="http://dbpedia.org/resource/The_Birds_and_the_Bees_II:_Antics">
    <ns2:publisher rdf:resource="http://dbpedia.org/resource/Bug-Byte"/>
    <ns2:name>The Birds and the Bees II: Antics</ns2:name>
    <ns2:hasPlatform rdf:resource="http://dbpedia.org/resource/Commodore_64"/>
    <ns2:hasGenre rdf:resource="http://dbpedia.org/resource/Action_game"/>
    <ns2:developer>Tim Lewis</ns2:developer>
